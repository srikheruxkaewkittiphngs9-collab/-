local function parseTime(str)
	local y,m,d,H,M,S = string.match(str,"(%d+)-(%d+)-(%d+) (%d+):(%d+):(%d+)")
	return os.time({year=tonumber(y),month=tonumber(m),day=tonumber(d),hour=tonumber(H),min=tonumber(M),sec=tonumber(S)})
end
local expireTime = parseTime("2025-10-31 23:59:59") 
local startTime = os.time()
local expired = false
local function valid() return os.time() <= expireTime and not expired end
local function rv(f)
    if valid() then f() 
    else StarterGui:SetCore("SendNotification",{Title="‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏õ‡∏¥‡∏î‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï",Text="‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏´‡∏°‡∏î‡∏≠‡∏≤‡∏¢‡∏∏‡πÅ‡∏•‡πâ‡∏ß",Duration=5}) 
    end
end

local DISCORD_LINK = "https://discord.gg/qpsEcM3j" 

local function showExpiredScreen()
    local gui = Instance.new("ScreenGui")
    gui.Name = "MosHub_Expired"
    gui.ResetOnSpawn = false
    gui.Parent = Player:WaitForChild("PlayerGui")

    local bg = Instance.new("Frame", gui)
    bg.Size = UDim2.new(1,0,1,0)
    bg.Position = UDim2.new(0,0,0,0)
    bg.BackgroundTransparency = 1

    local label = Instance.new("TextLabel", bg)
    label.Size = UDim2.new(1,0,0.2,0)
    label.Position = UDim2.new(0,0,0.35,0)
    label.BackgroundTransparency = 1
    label.Text = "‚ö†Ô∏è ‡∏£‡∏≠‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï ‚ö†Ô∏è"
    label.TextScaled = true
    label.TextColor3 = Color3.fromRGB(255,50,50)
    label.Font = Enum.Font.SourceSansBold

    local btn = Instance.new("TextButton", bg)
    btn.Size = UDim2.new(0,300,0,50)
    btn.Position = UDim2.new(0.5,-150,0.6,0)
    btn.Text = "‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏•‡∏¥‡∏á‡∏Å‡πå Discord"
    btn.TextScaled = true
    btn.Font = Enum.Font.SourceSansBold
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = Color3.fromRGB(30,144,255)
    btn.AutoButtonColor = true

    btn.MouseButton1Click:Connect(function()
        if setclipboard then
            pcall(function() setclipboard(DISCORD_LINK) end)
            StarterGui:SetCore("SendNotification",{Title="‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß",Text="‡∏•‡∏¥‡∏á‡∏Å‡πå Discord ‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß",Duration=4})
        else
            StarterGui:SetCore("SendNotification",{Title="‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏î‡πâ",Text="Executor ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡πÑ‡∏°‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö setclipboard",Duration=4})
        end
    end)
end

if os.time() > expireTime then
    showExpiredScreen()
    return
end

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/x2zu/OPEN-SOURCE-UI-ROBLOX/refs/heads/main/X2ZU%20UI%20ROBLOX%20OPEN%20SOURCE/DummyUi-leak-by-x2zu/fetching-main/Tools/Framework.luau"))()
local w = lib:Window({
    Title="Mos Hub | by ‡∏ö‡∏±‡∏á‡∏°‡∏≠‡∏™ ‡∏ô‡πâ‡∏∞‡∏à‡πâ‡∏∞",
    Desc="Mos Hub on top",
    Icon=105059922903197,
    Theme="Dark",
    Config={Keybind=Enum.KeyCode.P,Size=UDim2.new(0,700,0,520)},
    CloseUIButton={Enabled=true,Text="Mos Hub"}
})

local fpsText = Drawing.new("Text")
fpsText.Text = "FPS: 0"; fpsText.Size = 22; fpsText.Color = Color3.fromRGB(0,255,0); fpsText.Center = true; fpsText.Outline = true; fpsText.Visible = true
local dragging, mousePos, fpsPos = false, Vector2.new(), Vector2.new(150,50)
UserInputService.InputBegan:Connect(function(i)
    if expired then return end
    if i.UserInputType == Enum.UserInputType.MouseButton1 then
        local m = UserInputService:GetMouseLocation()
        if (Vector2.new(m.X,m.Y)-fpsPos).Magnitude <= 50 then
            dragging = true
            mousePos = Vector2.new(m.X,m.Y)-fpsPos
        end
    end
end)
UserInputService.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging=false end end)
UserInputService.InputChanged:Connect(function(i)
    if expired then return end
    if dragging and i.UserInputType == Enum.UserInputType.MouseMovement then
        local m = UserInputService:GetMouseLocation()
        fpsPos = Vector2.new(m.X,m.Y)-mousePos
    end
end)
local lastTick = tick()
RunService.RenderStepped:Connect(function()
    if expired then fpsText.Visible=false return end
    local now = tick()
    local fps = math.floor(1 / math.max((now-lastTick), 0.000001))
    lastTick = now
    fpsText.Text = "FPS: "..fps
    fpsText.Position = fpsPos
end)

local admin = w:Tab({Title="‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"})
admin:Section({Title="‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏≠‡∏î‡∏°‡∏¥‡∏ô"})
local infiniteJump, noclip = false, false

admin:Toggle({Title="‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î",Callback=function(v) if not expired then infiniteJump=v end end})
admin:Slider({Title="‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á",Min=0,Max=500,Value=50,Callback=function(v) if not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.JumpPower=v end end})
admin:Slider({Title="‡∏ß‡∏¥‡πà‡∏á‡πÑ‡∏ß",Min=0,Max=500,Value=16,Callback=function(v) if not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.WalkSpeed=v end end})
admin:Toggle({Title="‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏™‡∏¥‡πà‡∏á‡∏Ç‡∏≠‡∏á (Noclip)",Callback=function(v) if not expired then noclip=v end end})

RunService.Stepped:Connect(function()
    if noclip and not expired and Player.Character then
        for _,p in pairs(Player.Character:GetDescendants()) do
            if p:IsA("BasePart") then p.CanCollide=false end
        end
    end
end)

UserInputService.JumpRequest:Connect(function()
    if infiniteJump and not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then
        Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

admin:Button({Title="Boot FPS (‡∏•‡∏î‡∏Ñ‡∏∏‡∏ì‡∏†‡∏≤‡∏û)",Callback=function()
    if expired then return end
    Lighting.GlobalShadows = false
    Lighting.Brightness = 2
    Lighting.FogEnd = 1000
    Lighting.OutdoorAmbient = Color3.fromRGB(128,128,128)
    StarterGui:SetCore("SendNotification", {Title="Mos Hub", Text="Boot FPS ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡∏†‡∏≤‡∏û‡∏Å‡∏≤‡∏Å‡∏•‡∏á‡πÅ‡∏•‡πâ‡∏ß", Duration=4})
end})

admin:Button({Title="‡∏£‡∏±‡∏ô Infinite Yield",Callback=function() rv(function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end) end})

local es = w:Tab({Title="ESP"})
es:Section({Title="ESP Box/Line/Distance / ‡∏™‡∏¥‡∏á‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á"})
local eb,el,ed=false,false,false
es:Toggle({Title="Box",Callback=function(v) if not expired then eb=v end end})
es:Toggle({Title="Line",Callback=function(v) if not expired then el=v end end})
es:Toggle({Title="Distance",Callback=function(v) if not expired then ed=v end end})

local targetViewPlayer = nil
local isPossessing = false
local originalCameraType = nil
local originalCameraSubject = nil

es:Section({Title="‡∏™‡∏¥‡∏á‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á"})
local playerList = {}
local selectedPlayerName = nil
local function UpdatePlayerList()
    playerList = {}
    for _,pp in pairs(Players:GetPlayers()) do
        if pp ~= Player then table.insert(playerList, pp.Name) end
    end
end
UpdatePlayerList()
Players.PlayerAdded:Connect(UpdatePlayerList)
Players.PlayerRemoving:Connect(function(p) UpdatePlayerList() if selectedPlayerName == p.Name then selectedPlayerName = nil end end)

es:Dropdown({Title="‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏™‡∏¥‡∏á",List=playerList,Callback=function(name) selectedPlayerName = name end})
es:Button({Title="‡∏™‡∏¥‡∏á‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å",Callback=function()
    if expired then return end
    if not selectedPlayerName then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",Duration=3}) return end
    local pp = Players:FindFirstChild(selectedPlayerName)
    if not pp or not pp.Character or not pp.Character:FindFirstChild("HumanoidRootPart") then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°",Duration=3}) return end

    if isPossessing then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏™‡∏¥‡∏á‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß",Duration=3}) return end
    -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
    originalCameraType = Workspace.CurrentCamera.CameraType
    originalCameraSubject = Workspace.CurrentCamera.CameraSubject
    isPossessing = true
    targetViewPlayer = pp

    Workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
    local rsConn
    rsConn = RunService.RenderStepped:Connect(function()
        if not isPossessing or not targetViewPlayer or not targetViewPlayer.Character or not targetViewPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if rsConn then rsConn:Disconnect() end
            return
        end
        local root = targetViewPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root then
            -- ‡∏ß‡∏≤‡∏á‡∏Å‡∏•‡πâ‡∏≠‡∏á‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏®‡∏µ‡∏£‡∏©‡∏∞‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
            local cf = root.CFrame * CFrame.new(0, 1.5, -2) * CFrame.Angles(-0.1, 0, 0)
            Workspace.CurrentCamera.CFrame = cf
        end
    end)
    StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏™‡∏¥‡∏á‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á: "..selectedPlayerName,Duration=3})
end})

es:Button({Title="‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏™‡∏¥‡∏á",Callback=function()
    if not isPossessing then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡∏™‡∏¥‡∏á‡∏≠‡∏¢‡∏π‡πà",Duration=3}) return end
    isPossessing = false
    targetViewPlayer = nil
    -- ‡∏Ñ‡∏∑‡∏ô‡∏Å‡∏•‡πâ‡∏≠‡∏á‡πÄ‡∏î‡∏¥‡∏° (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
    if originalCameraType then Workspace.CurrentCamera.CameraType = originalCameraType end
    if originalCameraSubject and originalCameraSubject.Parent then Workspace.CurrentCamera.CameraSubject = originalCameraSubject end
    StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏™‡∏¥‡∏á‡πÅ‡∏•‡πâ‡∏ß",Duration=3})
end})

local DS = {}
local function ClearESP(plr)
    if DS[plr] then
        for _,o in pairs(DS[plr]) do
            if type(o.Remove) == "function" then
                pcall(function() o:Remove() end)
            end
        end
    end
    DS[plr] = nil
end
Players.PlayerRemoving:Connect(function(plr) ClearESP(plr) end)

RunService.RenderStepped:Connect(function()
    if expired then
        for _,d in pairs(DS) do for _,o in pairs(d) do if o and type(o.Visible)~="nil" then o.Visible=false end end end
        return
    end
    if not Player.Character or not Player.Character:FindFirstChild("HumanoidRootPart") then return end
    local cam = Workspace.CurrentCamera
    for _,pp in pairs(Players:GetPlayers()) do
        if pp~=Player and pp.Character and pp.Character:FindFirstChild("HumanoidRootPart") then
            local root = pp.Character.HumanoidRootPart
            local screenPos, onScreen = cam:WorldToViewportPoint(root.Position)
            local dist = (Player.Character.HumanoidRootPart.Position - root.Position).Magnitude
            if not DS[pp] then DS[pp] = {} end
            -- Box
            if eb then
                if not DS[pp].B then local b=Drawing.new("Square"); b.Thickness=1; b.Color=Color3.fromRGB(255,0,0); b.Filled=false; DS[pp].B=b end
                DS[pp].B.Size = Vector2.new(50,50)
                DS[pp].B.Position = Vector2.new(screenPos.X-25, screenPos.Y-25)
                DS[pp].B.Visible = onScreen
            elseif DS[pp].B then DS[pp].B.Visible=false end
            -- Line
            if el then
                if not DS[pp].L then local l=Drawing.new("Line"); l.Color=Color3.fromRGB(0,255,0); l.Thickness=1; DS[pp].L=l end
                DS[pp].L.From = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)
                DS[pp].L.To = Vector2.new(screenPos.X, screenPos.Y)
                DS[pp].L.Visible = onScreen
            elseif DS[pp].L then DS[pp].L.Visible=false end
            -- Distance
            if ed then
                if not DS[pp].D then local t=Drawing.new("Text"); t.Color=Color3.fromRGB(255,255,0); t.Size=14; t.Center=true; t.Outline=true; DS[pp].D=t end
                DS[pp].D.Text = string.format("%.1f", dist)
                DS[pp].D.Position = Vector2.new(screenPos.X, screenPos.Y-35)
                DS[pp].D.Visible = onScreen
            elseif DS[pp].D then DS[pp].D.Visible=false end
        end
    end
end)

local wp = w:Tab({Title="‡∏ß‡∏≤‡∏õ"})
wp:Section({Title="‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"})
local Wlist = {}
local selectedPlayer = nil
local function UpdateWlist()
    Wlist = {}
    for _,pp in pairs(Players:GetPlayers()) do
        if pp~=Player then table.insert(Wlist, pp.Name) end
    end
end
UpdateWlist()
Players.PlayerAdded:Connect(UpdateWlist)
Players.PlayerRemoving:Connect(UpdateWlist)
wp:Dropdown({Title="‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",List=Wlist,Callback=function(n) selectedPlayer = Players:FindFirstChild(n) end})
wp:Button({Title="‡∏¢‡∏∑‡∏ô‡∏¢‡∏±‡∏ô‡∏ß‡∏≤‡∏õ",Callback=function()
    if expired or not selectedPlayer then return end
    if Player.Character and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        Player.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame
    end
end})

local inv = w:Tab({Title="‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô"})
inv:Section({Title="‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô"})
inv:Toggle({Title="‡∏•‡πà‡∏≠‡∏á‡∏´‡∏ô",Callback=function(v)
    if expired then return end
    if Player.Character then
        for _,o in pairs(Player.Character:GetDescendants()) do
            if o:IsA("BasePart") and o.Name~="HumanoidRootPart" then
                o.Transparency = v and 1 or 0
                o.CanCollide = not v
            end
        end
    end
end})

