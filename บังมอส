local tt = w:Tab({Title="‡πÄ‡∏ß‡∏•‡∏≤‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå"})
tt:Section({Title="‡∏´‡∏°‡∏î‡∏≠‡∏≤‡∏¢‡∏∏"})
local lab = tt:Label({Title="‡∏´‡∏°‡∏î‡∏≠‡∏≤‡∏¢‡∏∏‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà 31/10/2025"})
tt:Label({Title="‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏¥‡πà‡∏°: "..os.date("%d/%m/%Y %H:%M:%S",startTime)})

spawn(function()
    while true do
        local rem = expireTime - os.time()
        if rem<=0 then
            lab:Set("‚ö†Ô∏è ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏´‡∏°‡∏î‡∏≠‡∏≤‡∏¢‡∏∏‡πÅ‡∏•‡πâ‡∏ß ‚ö†Ô∏è")
            if not expired then
                expired=true
                -- ‡∏õ‡∏¥‡∏î UI ‡πÅ‡∏•‡∏∞ overlay
                pcall(function() w:Close() end)
                fpsText.Visible = false
                -- ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡∏™‡∏¥‡∏á (‡∏ñ‡πâ‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏™‡∏¥‡∏á)
                isPossessing = false
                targetViewPlayer = nil
                if originalCameraType then Workspace.CurrentCamera.CameraType = originalCameraType end
                if originalCameraSubject and originalCameraSubject.Parent then pcall(function() Workspace.CurrentCamera.CameraSubject = originalCameraSubject end) end
              
                showExpiredScreen()
            end
        else
            local d = math.floor(rem/86400)
            local h = math.floor((rem%86400)/3600)
            local m = math.floor((rem%3600)/60)
            local s = rem%60
            lab:Set(string.format("‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏•‡∏∑‡∏≠: %02d ‡∏ß‡∏±‡∏ô %02d:%02d:%02d", d, h, m, s))
        end
        task.wait(1)
    end
end)
