local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local Player = Players.LocalPlayer

-- ‚úÖ GUI ‡∏´‡∏•‡∏±‡∏Å
local introGui = Instance.new("ScreenGui")
introGui.Name = "MosHubIntro"
introGui.IgnoreGuiInset = true
introGui.ResetOnSpawn = false
introGui.Parent = Player:WaitForChild("PlayerGui")

-- ‚òÅÔ∏è ‡∏û‡∏∑‡πâ‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏Ñ‡∏ß‡∏±‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß‡πÄ‡∏£‡∏∑‡∏≠‡∏á‡πÅ‡∏™‡∏á
local smoke = Instance.new("ImageLabel", introGui)
smoke.Size = UDim2.new(1, 0, 1, 0)
smoke.BackgroundTransparency = 1
smoke.Image = "rbxassetid://6885837595" -- texture ‡∏Ñ‡∏ß‡∏±‡∏ô
smoke.ImageTransparency = 0.5
smoke.ImageColor3 = Color3.fromRGB(0, 255, 100) -- üíö ‡∏Ñ‡∏ß‡∏±‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß
smoke.ScaleType = Enum.ScaleType.Tile
smoke.TileSize = UDim2.new(3, 0, 3, 0)
smoke.ZIndex = 0

task.spawn(function()
	while introGui.Parent do
		smoke.Position = smoke.Position + UDim2.new(0.001, 0, 0, 0)
		task.wait(0.02)
	end
end)

-- üåà ‡∏Å‡∏£‡∏≠‡∏ö‡∏™‡∏≤‡∏¢‡∏£‡∏∏‡πâ‡∏á‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠
local border = Instance.new("Frame", introGui)
border.AnchorPoint = Vector2.new(0.5, 0.5)
border.Position = UDim2.new(0.5, 0, 0.5, 0)
border.Size = UDim2.new(0, 520, 0, 220)
border.BackgroundTransparency = 1
border.ZIndex = 1

-- üåÄ ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏™‡∏µ‡∏£‡∏∏‡πâ‡∏á‡∏£‡∏≠‡∏ö‡∏Å‡∏£‡∏≠‡∏ö
local rainbow = Instance.new("UIGradient", border)
rainbow.Color = ColorSequence.new{
	ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
	ColorSequenceKeypoint.new(0.16, Color3.fromRGB(255, 127, 0)),
	ColorSequenceKeypoint.new(0.33, Color3.fromRGB(255, 255, 0)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 255, 0)),
	ColorSequenceKeypoint.new(0.66, Color3.fromRGB(0, 0, 255)),
	ColorSequenceKeypoint.new(0.83, Color3.fromRGB(75, 0, 130)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(148, 0, 211))
}
rainbow.Rotation = 0

-- ‡∏´‡∏°‡∏∏‡∏ô‡∏™‡∏µ‡∏£‡∏∏‡πâ‡∏á‡∏£‡∏≠‡∏ö‡∏Å‡∏£‡∏≠‡∏ö
task.spawn(function()
	while introGui.Parent do
		rainbow.Rotation = (rainbow.Rotation + 1) % 360
		task.wait(0.02)
	end
end)

-- ‡∏Å‡∏£‡∏≠‡∏ö‡πÄ‡∏£‡∏∑‡∏≠‡∏á‡πÅ‡∏™‡∏á
local borderStroke = Instance.new("UIStroke", border)
borderStroke.Thickness = 7
borderStroke.Color = Color3.fromRGB(255, 255, 255)
borderStroke.Transparency = 0.25
borderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

-- üå∏ ‡∏ï‡∏±‡∏ß‡∏´‡∏ô‡∏±‡∏á‡∏™‡∏∑‡∏≠ "‡∏ö‡∏±‡∏á‡∏°‡∏≠‡∏™‡∏Æ‡∏±‡∏ö"
local title = Instance.new("TextLabel", border)
title.AnchorPoint = Vector2.new(0.5, 0.5)
title.Position = UDim2.new(0.5, 0, 0.5, 0)
title.Size = UDim2.new(1, 0, 1, 0)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.FredokaOne
title.TextScaled = true
title.Text = ""
title.ZIndex = 2

-- ‡πÄ‡∏á‡∏≤‡πÄ‡∏£‡∏∑‡∏≠‡∏á‡πÅ‡∏™‡∏á‡∏ä‡∏°‡∏û‡∏π‡∏°‡πà‡∏ß‡∏á
local glow = Instance.new("UIStroke", title)
glow.Thickness = 3
glow.Color = Color3.fromRGB(255, 150, 255)
glow.Transparency = 0.4

-- ‚ú® ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏û‡∏¥‡∏°‡∏û‡πå‡πÑ‡∏ß‡∏Ç‡∏∂‡πâ‡∏ô (0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏ï‡πà‡∏≠‡∏ï‡∏±‡∏ß)
local text = "‡∏ö‡∏±‡∏á‡∏°‡∏≠‡∏™‡∏Æ‡∏±‡∏ö"
for i = 1, #text do
	title.Text = string.sub(text, 1, i)
	task.wait(0.1)
end

-- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡πÄ‡∏ö‡∏•‡∏≠‡πÅ‡∏™‡∏á
local blur = Instance.new("BlurEffect", Lighting)
for i = 1, 15 do
	blur.Size = i * 1.5
	borderStroke.Thickness = 5 + math.sin(i / 2)
	task.wait(0.02)
end

-- ‡πÅ‡∏™‡∏î‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏Ñ‡πâ‡∏≤‡∏á‡πÑ‡∏ß‡πâ 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
task.wait(2)

-- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏Ñ‡πà‡∏≠‡∏¢ ‡πÜ ‡∏à‡∏≤‡∏á‡∏´‡∏≤‡∏¢
for i = 1, 15 do
	smoke.ImageTransparency = 0.5 + i / 15
	title.TextTransparency = i / 15
	borderStroke.Transparency = i / 15
	blur.Size = math.max(0, blur.Size - 2)
	task.wait(0.04)
end

blur:Destroy()
introGui:Destroy()

